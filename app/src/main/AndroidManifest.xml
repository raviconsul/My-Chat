<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ravi.mychat">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/log"
        android:label="@string/app_name"
        android:roundIcon="@drawable/log"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".EditInfo"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".ChatList"
            android:label="@string/title_activity_chat_list"
            android:theme="@style/AppTheme" />
        <activity android:name=".ChatPage" />
        <activity android:name=".Developers"
            android:screenOrientation="portrait"/>
        <activity android:name=".myInfo"
            android:screenOrientation="portrait"></activity>
    </application>

</manifest>